<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.javalec.ex.Dao.PDao">

	<select id="list" resultType="com.javalec.ex.Dto.PDto.ProductDto">
		select * from product order by p_udate asc
	</select>
	<select id="productDetail" resultType="com.javalec.ex.Dto.PDto.ProductDto">
		select * from PRODUCT where P_CODE=#{param1}
	</select>


</mapper>